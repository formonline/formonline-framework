package formOnLine.msBeans;

import com.triangle.lightfw.ValueBean;



/**
 * @author S. Leridon
 *
 */
public class Template extends ValueBean{

    private static final long serialVersionUID = 1L;
    private int id =0;
    private int fid =0;
    private String sVal ="" ;
    private String name ="" ;
    private String type ="" ;
    
    
    public static String SESSION_NAME ="template" ;
    public static String SESSION_LIST ="liste_template" ;
    
    public static String MAIL ="MAIL" ;
    public static String HTML ="HTML" ;
    public static String CSV ="CSV" ;
    public static String RTF ="RTF" ;
    public static String MTO ="MTO" ;
    public static String SQL = "SQL";
    public static String TXT = "TXT";
    public static String LOT = "LOT";
    public static String PDF = "PDF";
    
    
    
    
    public Template () {
    }   
  
public Template (int vid, String s, int f_id, String txt, String type) {
  this.id=vid;
  this.sVal=s;
  this.fid=f_id;
  this.name=txt;
  this.type=type;
}
  

  /**
   * @return Returns the p_id.
   */
  public int getId() {
    return id;
  }
  /**
   * @param p_id The p_id to set.
   */
  public void setId(int vid) {
    this.id = vid;
  }

  /**
   * @return Returns the sVal.
   */
  public String getContent() {
    return sVal;
  }
  /**
   * @param val The sVal to set.
   */
  public void setContent(String val) {
    sVal = val;
  }
    /**
     * @return Returns the fid.
     */
    public int getFid() {
        return fid;
    }
    /**
     * @param fid The fid to set.
     */
    public void setFid(int fid) {
        this.fid = fid;
    }
    /**
     * @return Returns the text.
     */
    public String getName() {
        return name;
    }
    /**
     * @param text The text to set.
     */
    public void setName(String text) {
        this.name = text;
    }


    /**
     * @return Returns the type.
     */
    public String getType() {
        return type;
    }


    /**
     * @param type The type to set.
     */
    public void setType(String type) {
        this.type = type;
    }
}
